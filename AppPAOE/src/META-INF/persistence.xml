<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="APPAOE" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>co.edu.usbbog.paoe.model.Facultad</class>
    <class>co.edu.usbbog.paoe.model.Programa</class>
    <class>co.edu.usbbog.paoe.model.Psicologo</class>
    <class>co.edu.usbbog.paoe.model.ProgramasEstudiante</class>
    <class>co.edu.usbbog.paoe.model.Asesoria</class>
    <class>co.edu.usbbog.paoe.model.Paciente</class>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/paoe_db"/>
      <property name="javax.persistence.jdbc.user" value="paoe_db"/>
      <property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver"/>
      <property name="javax.persistence.jdbc.password" value="paoe_2020"/>
    </properties>
  </persistence-unit>
</persistence>
